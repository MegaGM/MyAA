{
  "name": "myaa",
  "version": "2.9.4",
  "license": "Apache-2.0",
  "description": "My Anime Assistant for MAL and Nyaa.si",
  "author": "Mega <Aza.Noriega@gmail.com>",
  "homepage": "https://bitbucket.org/Megga/horrible-nyaa-mal#readme",
  "main": "src/main/index.js",
  "scripts": {
    "app": "./node_modules/electron/dist/electron .",
    "dev": "NODE_ENV=development ./node_modules/electron/dist/electron .",
    "build": "./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack -w",
    "package:linux": "electron-packager ./ HorribleNyaaMAL --platform=linux --arch=x64 --overwrite --extra-resource='resources' --out='build'"
  },
  "build": {
    "appId": "org.AzaNoriega.MyAA",
    "productName": "My Anime Assistant",
    "files": [
      "main.js",
      "api.js",
      "src/**/*"
    ],
    "directories": {
      "output": "build"
    },
    "linux": {
      "target": [
        "deb"
      ],
      "icon": "resources/icons/png/icon.png",
      "category": "Science"
    },
    "win": {
      "icon": "resources/icons/win/icon.ico",
      "target": "portable"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/Megga/horrible-nyaa-mal.git"
  },
  "keywords": [
    "nyaa",
    "nyaa.si",
    "torrent",
    "ongoing",
    "myanimelist",
    "mal",
    "horriblesubs"
  ],
  "bugs": {
    "url": "https://bitbucket.org/Megga/horrible-nyaa-mal/issues"
  },
  "dependencies": {
    "ant-design-vue": "^1.3.3",
    "axios": "^0.18.0",
    "cron": "^1.6.0",
    "fast-xml-parser": "^3.12.12",
    "fs-extra": "^7.0.1",
    "mathjs": "^5.4.2",
    "puppeteer": "^1.11.0",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/puppeteer": "^1.11.1",
    "css-loader": "*",
    "electron": "^4.x.x",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^13.0.1",
    "file-loader": "*",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "mini-css-extract-plugin": "^0.5.0",
    "style-loader": "*",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "url-loader": "*",
    "vue": "^2.5.22",
    "vue-loader": "*",
    "vue-router": "^3.0.2",
    "vue-style-loader": "*",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
